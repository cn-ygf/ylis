<?xml version="1.0" encoding="UTF-8"?>
<!--page全高度462-->
<Window size="550,408" caption="0,0,0,26">
  <VBox bkcolor="bk_color">
    <!--标题栏-->
    <HBox height="26">
      <Control />
      <!--系统按钮-->
      <HBox width="68">
        <Button class="btn_min" name="minbtn" />
        <Button class="btn_close" name="closebtn" />
      </HBox>
    </HBox>
    <!--logo-->
    <Control  height="66"/>
    <HBox padding="0,0,0,0" height="80">
      <Control />
      <Label class="my_logo" />
      <Control />
    </HBox>
    <Control height="10" />
    <HBox padding="0,0,0,0" height="30">
      <Control />
      <Label class="my_title" text="{{title}}卸载中..."></Label>
      <Control />
    </HBox>
    <!-- tabbox-->
    <TabBox name="tb_main" selectedid="0">
      <!--卸载中-->
      <VBox>
        <Control height="40" />
        <HBox height="5">
          <Control />
          <VBox width="200"><Progress class="my_progress" name="progress" value="0" /></VBox>
          <Control />
        </HBox>
        <Control height="15" />
        <HBox>
          <Control />
          <Label class="my_subtitle" name="lb_progress" text="0%" />
          <Control />
        </HBox>
      </VBox>
      <!--卸载中结束-->
      <!--卸载完成-->
      <VBox>
        <Control height="40" />
        <HBox height="40">
          <Control />
          <Button class="btn_master" name="btn_success" text="卸载完成" width="180" height="30"/>
          <Control />
        </HBox>
      </VBox>
      <!--卸载完成结束-->
      <!--卸载失败-->
      <VBox>
        <Control height="40" />
        <HBox height="40">
          <Control />
          <Label class="my_title" text="卸载失败，请联系管理员"/>
          <Control />
        </HBox>
      </VBox>
      <!--卸载失败结束-->
    </TabBox>
  </VBox>
</Window>